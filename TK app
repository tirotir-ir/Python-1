import tkinter as tk
root = tk.Tk()
root.title('worden')
root.geometry('600x400')
def x():
    root = tk.Tk()
    root.title('login')
    root.geometry('600x400')
    nothing_lable = tk.Label(root,text="nothing was found")
    nothing_lable.pack()
def y():
    root = tk.Tk()
    root.title('lsign up')
    root.geometry('600x400')
    nothing_lable = tk.Label(root,text="Error")
    nothing_lable.pack()

def login():
    root = tk.Tk()
    root.title('log in')
    root.geometry('600x400')
    Email_lable = tk.Label(root,text="Email")
    Email_lable.pack()
    Email_Entry = tk.Entry(root)
    Email_Entry.pack()
    password_lable = tk.Label(root,text="password")
    password_lable.pack()
    password_Entry = tk.Entry(root)
    password_Entry.pack()
    rigester_butten = tk.Button(root,text='rigester',command=x)
    rigester_butten.pack()
def signup():
    root = tk.Tk()
    root.title('sign up')
    root.geometry('600x400')
    name_lable = tk.Label(root,text="name")
    name_lable.pack()
    name_Entry = tk.Entry(root)
    name_Entry.pack()
    nationality_lable = tk.Label(root,text="nationality")
    nationality_lable.pack()
    nationality_Entry = tk.Entry(root)
    nationality_Entry.pack()
    Email_lable = tk.Label(root,text="Email")
    Email_lable.pack()
    Email_Entry = tk.Entry(root)
    Email_Entry.pack()
    password_lable = tk.Label(root,text="password")
    password_lable.pack()
    password_Entry = tk.Entry(root)
    password_Entry.pack()
    signup_butten = tk.Button(root,text='sign up',command=y)
    signup_butten.pack()

''
Email_lable = tk.Label(root,text="Email")
Email_lable.pack()
Email_Entry = tk.Entry(root)
Email_Entry.pack()

password_lable = tk.Label(root,text="password")
password_lable.pack()
password_Entry = tk.Entry(root)
password_Entry.pack()

login_butten = tk.Button(root,text='log in',command=login)
login_butten.pack()
signup_butten = tk.Button(root,text='sign up',command=signup)
signup_butten.pack()
exit_butten = tk.Button(root,text='exit',command=root.destroy)
exit_butten.pack()

root.mainloop()
